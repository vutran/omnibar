{
  "name": "omnibar",
  "version": "1.1.0",
  "description": "Extensible search component for React.",
  "main": "./dist/index.js",
  "types": "./typings/index.d.ts",
  "files": ["dist", "typings", "LICENSE", "README.md"],
  "repository": {
    "type": "git",
    "url": "https://github.com/vutran/omnibar.git"
  },
  "scripts": {
    "build": "rollup -c",
    "format": "npm run format:js && npm run format:md",
    "format:js":
      "prettier --write '{*.js,*.json,{src,__tests__,typings}/**/*.ts{,x}}'",
    "format:md": "prettier --parser markdown --write '*.md'",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "jest": "jest --coverage --verbose",
    "test": "npm run jest"
  },
  "lint-staged": {
    "{*.js,{src,__tests__,typings}/**/*.ts{,x}}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": ["omnibar", "search", "navigator"],
  "author": "Vu Tran <vu@vu-tran.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "@types/jest": "^21.1.1",
    "@types/node": "^8.0.31",
    "@types/react": "^15.0.24",
    "@types/react-dom": "^15.5.0",
    "@types/react-test-renderer": "^15.5.0",
    "@types/webspeechapi": "^0.0.27",
    "babel-jest": "^21.2.0",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^4.2.3",
    "prettier": "^1.9.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "regenerator-runtime": "^0.11.0",
    "rollup": "^0.50.0",
    "rollup-plugin-typescript": "^0.8.1",
    "ts-jest": "^21.0.1",
    "ts-loader": "^2.3.7",
    "typescript": "^2.5.3"
  },
  "jest": {
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  },
  "dependencies": {}
}
